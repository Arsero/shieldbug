{
	"name": "api",
	"version": "1.0.0",
	"description": "A bug tracker made with NodeJs and Angular",
	"main": "index.js",
	"scripts": {
		"dev": "ts-node-dev --respawn --pretty --transpile-only src/app.ts",
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "ts-node src/app.ts",
		"add-migration": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
		"update-database": "ts-node ./node_modules/typeorm/cli.js migration:run"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Arsero/shieldbug.git"
	},
	"author": "Azzedine",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Arsero/shieldbug/issues"
	},
	"homepage": "https://github.com/Arsero/shieldbug#readme",
	"dependencies": {
		"@automapper/classes": "^3.5.2",
		"@automapper/core": "^3.5.2",
		"bcrypt": "^5.0.1",
		"class-validator": "^0.13.1",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-kun": "^1.0.24",
		"helmet": "^4.4.1",
		"jsonwebtoken": "^8.5.1",
		"pg": "^8.5.1",
		"reflect-metadata": "^0.1.13",
		"typeorm": "0.2.32",
