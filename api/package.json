		"@types/express": "^4.17.11",
		"@types/helmet": "^4.0.0",
		"@types/jsonwebtoken": "^8.5.1",
		"@types/node": "^8.0.29",
		"@types/uuid-validate": "^0.0.1",
		"ts-node": "3.3.0",
		"ts-node-dev": "^1.1.6",
		"typescript": "3.3.3333"
	}
